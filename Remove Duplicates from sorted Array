

class Solution:
    def removeDuplicates(self, nums: List[int]) -> int:

        
        i = 0
        while (i < (len(nums))):
            j = i+1
            while (j < len(nums) and nums[i] == nums[j]):
                nums.pop(j)
            i += 1
        return len(nums) 

###Another way to solve this problem

class Solution:
    def removeDuplicates(self, nums: List[int]) -> int:

        start = 0
        n = len(nums)
        for i in range(1,n):
            if nums[i] != nums[start]:
                start+=1
                nums[start] = nums[i]
        return start+1
