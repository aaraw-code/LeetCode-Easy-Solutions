##Array Simulation / Brute Force using List##
class Solution:
    def singleNumber(self, nums: List[int]) -> int:
        ans = []
        for i in range(len(nums)):
            
            if nums[i] in ans:
                ans.remove(nums[i])
            else:
                ans.append(nums[i])

        return ans[0]


## By XOR method
#a ^ a = 0
#a ^ 0 = a

class Solution:
    def singleNumber(self, nums: List[int]) -> int:
        result = 0
        for num in nums:
            result ^= num
        return result
