# implement valid parentheses by stack method
class Solution:
    def isValid(self, s: str) -> bool:
        n = len(s)
        if n%2 == 1:
            return False
        
        st = []
        for ch in list(s):
            if ch == "(" or ch == "{" or ch == "[":
                st.append(ch)
            else:
                if len(st) == 0:
                    return False
                top = st.pop()
                if ch == ")" and top!="(":
                    return False
                if ch == "]" and top!="[":
                    return False
                if ch == "}" and top!="{":
                    return False
        return len(st) == 0

#second Way to implement Valid Parenthesis
class Solution:
    def isValid(self, s: str) -> bool:
        char = {
            "(" : ")",
            "{" : "}",
            "[" : "]"
        }

        stack = []

        for c in s:
            if c in char:
                stack.append(c)
            else:
                if  not  stack:
                    return False
            
                top = stack.pop()
                if char[top] != c:
                    return False
        return not stack


                    return False
                if ch == "}" and top!="{":
                    return False
        return len(st) == 0
